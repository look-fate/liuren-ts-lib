{
  "name": "liuren-ts-lib",
  "version": "1.3.1",
  "author": "Coaixy <coaixy@outlook.com>",
  "license": "Apache 2.0",
  "description": "LiuRen TypeScript Library Made by LetFate",
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "esbuild": "^0.25.8",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "jest": "^30.0.5",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "scripts": {
    "build": "pnpm build:esm && pnpm build:cjs && pnpm build:types",
    "build:esm": "esbuild src/index.ts --bundle --platform=node --target=es2020 --format=esm --outfile=dist/esm/index.js --sourcemap=inline",
    "build:cjs": "esbuild src/index.ts --bundle --platform=node --target=es2020 --format=cjs --outfile=dist/cjs/index.js --sourcemap=inline",
    "build:types": "tsc --emitDeclarationOnly --outDir dist/types",
    "dev": "tsc -w",
    "start": "pnpm build && node --enable-source-maps dist/cjs/index.js",
    "test": "jest",
    "ts": "ts-node src/index.ts",
    "lint": "eslint src/**/*.ts",
    "fix": "eslint src/**/*.ts --fix",
    "prepare": "pnpm build"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/let-fate/liuren-ts-lib.git"
  },
  "dependencies": {
    "jest-util": "^30.0.5",
    "liuren-ts-lib": "^1.0.1",
    "tyme4ts": "^1.3.4"
  }
}
